{% extends "layout.html.twig" %}

{% block content %}
<div class="row">
  <div class="col-xs-12 col-md-7 col-lg-8">
    <div class="box-body">
      <b>Assets Table</b>
    </div>
  </div>
  <div class="col-xs-12 col-md-5 col-lg-4">

      <div class="box box-primary">
        <div class="box box-header"><h4>Upload assets in excell form</h4></div>
          <form id="#upload_form" >
            <div class="box-body">
                <input type="file" name="data_assets"/>
            </div>
          </form>
      </div>

      <div class="box box-primary">
        <div class="box box-header"><h4>Uploaded Versions</h4></div>
        <div class="box-body">
          <div class="input-group input-group-sm">
            <input id="#search" type="text" placeholder="Search for version" class="form-control">
            <span class="input-group-btn">
              <button type="button" class="btn btn-info btn-flat"><i class="fa fa-search" aria-hidden="true"></i></button>
            </span>
          </div>
        </div>
      </div>
  </div>
</div>

{% endblock %}
